{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "noImplicitAny": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "declaration": true,
    "outDir": "tmp/es5",
    "sourceMap": false,
    "moduleResolution": "node",
    "rootDir": ".",
    "baseUrl": ".",
    "paths": {
      "@angular/service-worker": ["src/"],
      "@angular/service-worker/worker": ["src/worker/"]
    },
    "lib": [
      "es2015",
      "dom"
    ],
    "types": [
      "jasmine",
      "protractor",
      "node",
      "selenium-webdriver",
      "whatwg-fetch"
    ]
	},
  "exclude": [
    "node_modules"
  ]
}