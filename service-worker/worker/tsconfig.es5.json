{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "noImplicitAny": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "declaration": true,
    "outDir": "tmp/es5",
    "sourceMap": true,
    "moduleResolution": "node",
    "rootDir": ".",
    "baseUrl": ".",
    "paths": {
      "@angular/service-worker": ["src/"],
      "@angular/service-worker/worker": ["src/worker/"]
    },
    "lib": [
      "dom",
      "es2015"
    ],
    "types": [
      "jasmine",
      "node",
      "protractor",
      "selenium-webdriver",
      "whatwg-fetch"
    ]
	},
  "files": [
    "src/webpack/index.ts",
    "src/worker/index.ts",
    "src/test/unit/worker.spec.ts",
    "src/test/e2e/harness/client/main.ts",
    "src/test/e2e/harness/server/server.ts",
    "src/test/e2e/spec/sanity.e2e.ts",
    "src/testing/index.ts",
    "src/testing/mock.spec.ts",
    "src/typings/jshashes.d.ts",
    "src/typings/service-worker.d.ts"
  ]
}